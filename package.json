{
  "name": "varlet-app-template-lite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:test": "vite build --mode test",
    "bump": "pnpm update @varlet/* @configurajs/* rattail --latest",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --allow-empty --concurrent false",
    "commit-msg": "pnpm exec commitlint -e $1"
  },
  "lint-staged": {
    "*.${vue,ts,tsx,js,jsx,less,css}": "pnpm format",
    "*.{vue,ts,tsx,js,jsx}": "pnpm lint"
  },
  "dependencies": {
    "@varlet/axle": "^0.10.1",
    "@varlet/touch-emulator": "^3.8.3",
    "@varlet/ui": "^3.8.3",
    "@vueuse/core": "^10.5.0",
    "dayjs": "^1.11.8",
    "pinia": "^2.1.3",
    "rattail": "^1.0.18",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@configurajs/eslint": "^0.1.0",
    "@configurajs/prettier": "^0.1.1",
    "@iconify-json/mdi": "^1.1.54",
    "@types/node": "^20.2.5",
    "@unocss/preset-rem-to-px": "^0.58.2",
    "@varlet/import-resolver": "^3.8.3",
    "@varlet/preset-unocss": "3.8.3",
    "@varlet/unplugin-icon-builder": "^0.6.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "eslint": "^9.17.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.2",
    "prettier": "^3.4.2",
    "simple-git-hooks": "^2.8.1",
    "typescript": "5.0.2",
    "unocss": "^0.58.2",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.0.10",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^2.1.10"
  },
  "packageManager": "pnpm@9.1.1",
  "engines": {
    "pnpm": ">=9.0"
  }
}
