{
  "name": "varlet-app-template-lite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "dev": "vite",
    "build": "vite build",
    "build:test": "vite build --mode test",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "type-check": "vue-tsc --noEmit",
    "bump": "pnpm update @varlet/touch-emulator @varlet/ui @varlet/use @varlet/axle"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --allow-empty --concurrent false",
    "commit-msg": "pnpm exec commitlint -e $1"
  },
  "lint-staged": {
    "*.${vue,ts,tsx,js,jsx,less,css}": "pnpm format",
    "*.{vue,ts,tsx,js,jsx}": "pnpm lint"
  },
  "dependencies": {
    "@varlet/axle": "^0.4.0",
    "@varlet/touch-emulator": "^2.20.4",
    "@varlet/ui": "^2.20.4",
    "@varlet/use": "^2.20.4",
    "dayjs": "^1.11.8",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@iconify-json/mdi": "^1.1.54",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@unocss/preset-rem-to-px": "^0.55.6",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "less": "^4.1.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "typescript": "5.0.2",
    "unocss": "^0.55.6",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.9",
    "vite-plugin-compression2": "^0.10.1",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-progress": "^0.0.7",
    "vue-tsc": "^1.6.5"
  }
}
